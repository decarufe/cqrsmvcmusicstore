@{
    ViewBag.Title = "Index";
}

<table>
    <tr>
        <th>
            Album Name
        </th>
        <th>
            Retraits
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model.Items)
    {
        <tr>
            <td>
                @item.AlbumName
            </td>
            <td>
                @item.Count
            </td>
            <td>
                <a href="#" class="RemoveLink" data-id="">Offrir un rabais</a>
            </td>
        </tr>
    }
</table>
